<p id="notice"><%= notice %></p>

<h1>Assets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @holdings.each do |holding| %>
      <tr>
        <td><%= link_to holding.file_name, download_url(holding) %></td>
        &nbsp;&nbsp;&nbsp;
        <td><%= link_to 'Show', holding %></td>
        <td><%= link_to 'Edit', edit_holding_path(holding) %></td>
        <td><%= link_to 'Destroy', holding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="asset_list"> 
      <div id="menu"> 
          <ul id= "top_menu">     
             <li><%= link_to "Upload", new_holding_path %></li> 
         </ul> 
      </div> 
</div>